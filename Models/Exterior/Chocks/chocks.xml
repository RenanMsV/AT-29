	<!-- Front fit -->

	<animation>
	  <object-name>FrontFit</object-name>
	  <type>rotate</type>
	  <factor>1</factor>
	  <expression>
			<product>
				<property>environment/windsock/wind-speed-kt</property>
				<sin>
					<difference>
						<property>environment/wind-from-heading-deg</property>
						<property>orientation/heading-deg</property>
					</difference>
				</sin>	
			</product>
		</expression>
	  <center>
	  	<x-m>-2.05091</x-m>
	  	<y-m>-0.45677</y-m>
	  	<z-m>-0.603273</z-m>
	  </center>
	  <axis>
	  	<x>0</x>
	  	<y>1</y>
	  	<z>0</z>
	  </axis>
	</animation>


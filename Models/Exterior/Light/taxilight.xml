<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
  <path>flash.ac</path>
  <animation>
    <type>noshadow</type>
    <object-name>flash</object-name>
  </animation>
  <animation>
    <type>alpha-test</type>
    <alpha-factor>0.01</alpha-factor>
  </animation>
  <animation>
    <type>select</type>
    <object-name>flash</object-name>
    <condition>
      <property>controls/lighting/taxi-light</property>
    </condition>
  </animation>
  <animation>
    <type>flash</type>
    <object-name>flash</object-name>
    <center>
      <x-m>0.0</x-m>
      <y-m>0.0</y-m>
      <z-m>0.0</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <power>64</power>
    <factor>6</factor>
    <offset>0</offset>
    <two-sides type="bool">false</two-sides>
    <min>0.3</min>
    <max>1</max>
  </animation>
</PropertyList>
    <object-name>LElevator</object-name>
    <object-name>HTailStrake</object-name>
    <object-name>sidescoop</object-name>
    <object-name>WindscreenFrame</object-name>
    <object-name>RMainGearDoorOuter</object-name>
    <object-name>NoseGearDoor</object-name>
    <object-name>LMainGearDoorOuter</object-name>
    <object-name>DorsalStrake</object-name>
    <object-name>DorsalScoop</object-name>
    <object-name>CanopyFrame</object-name>
    <object-name>LFuelTank</object-name>
    <object-name>RFuelTank</object-name>
  </effect>
  <!-- Aplying glass reflection effect to the canopy group -->
  <effect>
    <inherits-from>Aircraft/AT-29/Models/Effects/glass_reflection_effect/reflectglas-uber</inherits-from>
    <object-name>CanopyGlass</object-name>
    <object-name>WindscreenGlass</object-name>
  </effect>
  <!-- Aplying transparent effect to the prop so its Rembrandt ready. -->
  <effect>
   <inherits-from>Effects/model-transparent</inherits-from>
   <object-name>PropDisk</object-name>
  </effect>
  <!-- Adding animated airshow smoke particle emitter -->
  <model>
    <path>Aircraft/AT-29/Models/Effects/airshow_smoke/smokeW.xml</path>
    <offsets>
      <x-m>-2.79324</x-m>
      <y-m>0.646238</y-m>
      <z-m>0.0</z-m>
      <roll-deg>0</roll-deg>
      <pitch-deg>0</pitch-deg>
      <heading-deg>0</heading-deg>
    </offsets>
  </model>
  <!-- Adding animated Pilots -->
  <model>
    <name>Pilot-fwd</name>
    <path>Aircraft/AT-29/Models/Interior/Pilots/Pilot.xml</path>
    <condition>
      <not>
        <property>sim/current-view/internal</property>
      </not>
    </condition>
    <offsets>
      <x-m>0.17</x-m>
      <y-m>-0.01</y-m>
      <z-m>0.1</z-m>
    </offsets>
  </model>
  <model>
    <name>Pilot-aft</name>
    <path>Aircraft/AT-29/Models/Interior/Pilots/Pilot.xml</path>
    <condition>
      <and>
        <not>
          <property>sim/model/mono-cockpit-enabled</property>
        </not>
        <not>
          <property>sim/current-view/internal</property>
        </not>
      </and>
    </condition>
    <offsets>
      <x-m>1.57</x-m>
      <y-m>-0.01</y-m>
      <z-m>0.23</z-m>
    </offsets>
  </model>
  <!-- Adding animated chocks and protections -->
  <model>
    <name>ChocksAndProtections</name>
    <path>Aircraft/AT-29/Models/Exterior/Chocks/chocks.xml</path>
    <condition>
      <property>/sim/model/chocks-enabled</property>
    </condition>
  </model>
  <animation>
    <type>select</type>
    <object-name>gunchocks</object-name>
    <condition>
      <property>/sim/model/livery/armed</property> <!--Hide gun chocks if not military-->
    </condition>
  </animation>
  <animation>
    <type>pick</type>
    <object-name>ChocksAndProtections</object-name> <!-- Disable cocks if clicked -->
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>/sim/model/chocks-enabled</property>
      </binding>
    </action>
  </animation>
</PropertyList>
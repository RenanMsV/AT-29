<?xml version="1.0" encoding="UTF-8"?>
<!-- A wrapper file for the Tucano 3D model -->
<PropertyList>
  <path>tucano.ac</path>
  <nasal>
    <load>
      <!--
      ##############################################################################
      # The on-load Nasal is not executed when this file is loaded as the user
      # aircraft. The code is only executed if the file is loaded for a remote
      # aircraft in Multi-Player mode (MP)
      ##############################################################################
      -->
      <![CDATA[var livery_update = aircraft.livery_update.new("Aircraft/AT-29/Models/Liveries");]]>
    </load>
    <unload><![CDATA[livery_update.stop();]]></unload>
  </nasal>
  <sound>
    <path>Aircraft/AT-29/Sounds/tucano-mp-sound.xml</path>
  </sound>
  <!-- Here add the 3d/2d instruments -->
  <model>
    <path>Aircraft/AT-29/Models/Interior/Instruments/CHDD1/CHDD.xml</path>
    <name>chdd2-1</name>
    <offsets>
      <x-m>-1.142</x-m>
      <y-m>-0.253</y-m>
      <z-m>0.4765</z-m>
    </offsets>
  </model>
  <model>
    <path>Aircraft/AT-29/Models/Interior/Instruments/CHDD2/CHDD.xml</path>
    <name>chdd2-2</name>
    <offsets>
      <x-m>-1.157</x-m>
      <y-m>0.063</y-m>
      <z-m>0.555</z-m>
    </offsets>
  </model>
  <!-- Here add the 3d/2d effects -->
  <model>
    <path>Aircraft/AT-29/Models/Exterior/Davepack/Vortex/condensation-left.xml</path>
    <offsets>
      <x-m>1.0</x-m>
      <y-m>-5.8</y-m>
      <z-m>0.0</z-m>
    </offsets>
  </model>
  <model>

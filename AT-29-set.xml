<?xml version="1.0" encoding="UTF-8"?>
<!--
************************************************************************
Super Tucano A-29B config file
David Culp, Alpha Simulations, Piglet, pjedvaj, RenanMsV, Kaito
************************************************************************
-->
<PropertyList>
  <sim>
    <description>Embraer Super Tucano A-29B, FAB</description>
    <author>David Culp, Alpha Simulations, Piglet, pjedvaj, RenanMsV, Kaito</author>
    <status>alpha</status>
    <flight-model>jsb</flight-model>
    <aero>super-tucano-FDM</aero>
    <startup>
      <splash-texture>Aircraft/AT-29/Splashs/splash.jpg</splash-texture>
      <splash-texture>Aircraft/AT-29/Splashs/splash01.jpg</splash-texture>
      <splash-texture>Aircraft/AT-29/Splashs/splash02.jpg</splash-texture>
      <splash-texture>Aircraft/AT-29/Splashs/splash03.jpg</splash-texture>
    </startup>
    <previews>
      <preview>
        <type>exterior</type>
        <splash type="bool">true</splash>
        <path>Splashs/splash.jpg</path>
      </preview>
      <preview>
        <type>exterior</type>
        <splash type="bool">true</splash>
        <path>Splashs/splash01.jpg</path>
      </preview>
      <preview>
        <type>exterior</type>
        <splash type="bool">true</splash>
        <path>Splashs/splash02.jpg</path>
      </preview>
      <preview>
        <type>exterior</type>
        <splash type="bool">true</splash>
        <path>Splashs/splash03.jpg</path>
      </preview>
    </previews>
    <systems>
      <electrical/>
    </systems>
    <sound>
      <path>Aircraft/AT-29/Sounds/tucano-sound.xml</path>
    </sound>
    <submodels>
      <serviceable type="bool">true</serviceable>
      <path>Aircraft/AT-29/submodels.xml</path>
    </submodels>
    <virtual-cockpit archive="y">true</virtual-cockpit>
    <allow-toggle-cockpit archive="y">false</allow-toggle-cockpit>
    <model>
      <path archive="y">Aircraft/AT-29/Models/AT-29.xml</path>
      <livery>
        <file type="string" />
      </livery>
    </model>
    <view n="0">
      <internal archive="y">true</internal>
      <config>
        <pitch-offset-deg>-4.0</pitch-offset-deg>
        <x-offset-m archive="y">0.0</x-offset-m> <!-- right/left -->
        <y-offset-m archive="y">0.68</y-offset-m> <!-- up/down -->
        <z-offset-m archive="y">-0.40</z-offset-m> <!-- aft/fore -->
      </config>
    </view>
    <chase-distance-m type="double" archive="y">-15.0</chase-distance-m>
    <flaps>
      <setting>0.00</setting>
      <setting>0.49</setting>
      <setting>1.00</setting>
    </flaps>
    <help>
      <key>
        <name>Shift-Q</name>
        <desc>fuel cutoff switch or lever</desc>
      </key>
      <key>
        <name>m (lower case m) </name>
        <desc>Nose wheel steering</desc>
      </key>
      <key>
        <name>c (lower case c) </name>
        <desc>Open/Close canopy</desc>
      </key>
      <key>
        <name>s (lower case s)</name>
        <desc>Toggle starter valves</desc>
      </key>
      <key>
        <name>o (lower case o)</name>
        <desc>Toggle smoke</desc>
      </key>
    </help>
    <rendering>
      <redout>
        <enabled type="bool" userarchive="y">true</enabled>
        <parameters>
          <!-- Blackscreen by G-Force limits -3.5 to 7, but unused -->
          <blackout-onset-g>6.5</blackout-onset-g>
          <blackout-complete-g>7</blackout-complete-g>
          <redout-onset-g>-3</redout-onset-g>
          <redout-complete-g>-3.5</redout-complete-g>
        </parameters>
      </redout>
      <AT-29>
        <glass-reflect>
          <amb-correction>1</amb-correction>
          <correction>1</correction>
          <dynamic>1</dynamic>
          <fresnel>0</fresnel>
          <noise>0</noise>
          <rainbow>-.5</rainbow>
        </glass-reflect>
      </AT-29>
    </rendering>
    <multiplay>
      <generic>
        <int n="0" type="int">0</int>
        <int n="1" type="int">0</int>
        <int n="2" type="int">0</int>
        <int n="3" type="int">0</int>
        <int n="4" type="int">0</int> <!-- Vortex particles.xml -->
        <int n="5" type="int">0</int> <!-- Contrail particles.xml -->
        <int n="6" type="int" alias="/controls/smoke">0</int> <!-- Airshow smoke smokeW.xml -->
        <int n="7" type="int">0</int>
        <int n="8" type="int">0</int>
        <int n="9" type="int">0</int>
        <int n="10" type="int">0</int>
        <int n="11" type="int">0</int>
        <int n="12" type="int">0</int>
        <float n="0" type="float" alias="/engines/engine[0]/n2" /> <!-- beacon light systems/beacon.xml -->
        <float n="1" type="float" alias="/engines/engine[0]/starter" /> <!-- steer-cmd-norm NWS.xml -->
        <float n="2" type="float" alias="/gear/gear[0]/steering-norm" /> <!-- steering AT-29.xml -->
      </generic>
    </multiplay>
    <menubar>
      <default>
        <menu n="100">
          <label>AT-29 Super Tucano</label>
          <enabled type="bool">true</enabled>
          <item>
            <label>Select Livery</label>
            <binding>
              <command>nasal</command>
              <script>aircraft.livery.dialog.open();</script>
            </binding>
          </item>
          <item>
            <name>lights</name>
            <label>Lights</label>
            <enabled type="bool">0</enabled>
            <binding>
              <command>nasal</command>
              <script>
                var lights = gui.Dialog.new("sim/gui/dialogs/lights/dialog", "Aircraft/AT-29/Dialogs/lights-dlg.xml");
                lights.open();
              </script>
            </binding>
          </item>
        </menu>
      </default>
    </menubar>
    <tags>
      <tag>interceptor</tag>
      <tag>reconnaissance</tag>
      <tag>trainer</tag>
      <tag>aerobatic</tag>
      <tag>embraer</tag>
      <tag>1990s</tag>
      <tag>2000s</tag>
      <tag>retractable-gear</tag>
      <tag>glass-cockpit</tag>
      <tag>ifr</tag>
      <tag>pressurised</tag>
      <tag>rembrandt</tag>
      <tag>turboprop</tag>
      <tag>1-engine</tag>
    </tags>
    <rating>
      <FDM type="int">0</FDM>
      <systems type="int">1</systems>
      <cockpit type="int">2</cockpit>
      <model type="int">4</model>
    </rating>
  </sim>
  <controls>
    <flight>
      <elevator-trim>-0.07</elevator-trim>
    </flight>
    <lighting>
      <instruments-norm>0.3</instruments-norm>
    </lighting>
    <armament>
      <gunsight>
        <power-on type="bool">true</power-on>
        <intensity type="double">1.0</intensity>
      </gunsight>
    </armament>
    <gear>
      <brake-parking type="double">1</brake-parking>
    </gear>
    <smoke type="int" />
  </controls>
  <engines>
    <engine n="0">
      <running>false</running>
    </engine>
  </engines>
  <input>
    <keyboard>
      <key n="99">
        <name>c</name>
        <desc>toggle canopy</desc>
        <binding>
          <command>property-toggle</command>
          <property>fdm/jsbsim/systems/canopy/command</property>
        </binding>
      </key>
      <key n="109">
        <name>m</name>
        <desc>engage nose wheel steering</desc>
        <binding>
          <command>property-assign</command>
          <property>fdm/jsbsim/systems/NWS/engaged</property>
          <value>1</value>
        </binding>
        <mod-up>
          <binding>
            <command>property-assign</command>
            <property>fdm/jsbsim/systems/NWS/engaged</property>
            <value>0</value>
          </binding>
        </mod-up>
      </key>
      <key n="111">
        <name>o</name>
        <desc>toggle smoke</desc>
        <binding>
          <command>property-toggle</command>
          <property>/sim/multiplay/generic/int[6]</property>
        </binding>
      </key>
      <key n="115">
        <name>s</name>
        <desc>Toggle starter valves.</desc>
        <binding>
          <command>property-toggle</command>
          <property>controls/engines/engine[0]/starter</property>
        </binding>
      </key>
      <key n="81">
        <name>Shift-Q</name>
        <desc>Fuel Cutoff Switch</desc>
        <binding>
          <condition>
            <property>/sim/input/selected/engine[0]</property>
          </condition>
          <command>property-toggle</command>
          <property>/controls/engines/engine[0]/cutoff</property>
        </binding>
      </key>
    </keyboard>
  </input>
  <instrumentation>
    <marker-beacon>
      <volume type="double" archive="y">0.0</volume>
    </marker-beacon>
    <rdmi-mode type="int">0</rdmi-mode> <!-- 0=TAC, 1=VOR, 2=ADF -->
  </instrumentation>
  <fdm>
    <jsbsim>
      <systems>
        <NWS>
          <engaged type="double">0</engaged>
        </NWS>
        <canopy>
          <command type="double">0</command>
        </canopy>
        <armament>
          <station1>
            <selected type="double">0</selected>
          </station1>
          <station2>
            <selected type="double">0</selected>
          </station2>
          <station3>
            <selected type="double">0</selected>
          </station3>
        </armament>
      </systems>
    </jsbsim>
  </fdm>
  <station-selected type="int">1</station-selected>
  <canopy>
    <position-norm type="double">0</position-norm>
  </canopy>
  <nasal>
    <at29>
      <file>Aircraft/AT-29/Nasal/liveries.nas</file>
    </at29>
  </nasal>
</PropertyList>